<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:movie="http://example.org/movie_ontology#">

    <owl:Ontology rdf:about="http://example.org/movie_ontology"/>

    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/movie_ontology#Movie"/>
    <owl:Class rdf:about="http://example.org/movie_ontology#Genre"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/movie_ontology#belongs_to">
        <rdfs:domain rdf:resource="http://example.org/movie_ontology#Movie"/>
        <rdfs:range rdf:resource="http://example.org/movie_ontology#Genre"/>
    </owl:ObjectProperty>

</rdf:RDF>
